# ./.setsymlistの形式
# 基本的にはln -sに使う引数と同じで
# default "シンボリックリンクの元のパス" "貼りたい先のパス"
# このようにdefaultには"と"で区切らずにパスだけを"と"で区切ってください
# コメントは行頭に#です。それ以外は受け付けません
# default以外の識別名は./.setsymfunc.shの${SYM_FUNCLIST}を参照してください

default "./misc/default/XDG_CONFIG_HOME/fontconfig" "${XDG_CONFIG_HOME}/"
default "./misc/default/XDG_CONFIG_HOME/awesome" "${XDG_CONFIG_HOME}/"
default "./misc/default/HOME/.vimperator" "${HOME}/"
default "./misc/default/HOME/.mplayer" "${HOME}/"
default "./misc/default/HOME/.emacs" "${HOME}/"
default "./misc/default/HOME/.tmux.d" "${HOME}/"
default "./misc/default/HOME/.Xresources" "${HOME}/"
default "./misc/default/HOME/.xinitrc" "${HOME}/"
default "./misc/default/HOME/.curlrc" "${HOME}/"
default "./misc/default/HOME/.muttrc" "${HOME}/"
default "./misc/default/HOME/.vim" "${HOME}/"
default "./misc/default/HOME/.tmux.conf" "${HOME}/"
default "./misc/default/HOME/.mailcap" "${HOME}/"
default "./misc/default/HOME/.vimrc" "${HOME}/"
default "./misc/default/HOME/.Xmodmap" "${HOME}/"
default "./misc/default/HOME/.urlview" "${HOME}/"
default "./misc/default/HOME/.fbtermrc" "${HOME}/"
default "./misc/default/HOME/.uim" "${HOME}/"
default "./misc/default/HOME/.screenrc" "${HOME}/"
default "./misc/default/HOME/.xprofile" "${HOME}/"
default "./misc/default/HOME/.proxychains" "${HOME}/"
default "./misc/default/HOME/.wgetrc" "${HOME}/"
default "./misc/default/HOME/.gdbinit" "${HOME}/"
default "./misc/default/HOME/.gitconfig" "${HOME}/"
default "./misc/default/HOME/.vimperatorrc" "${HOME}/"
default "./misc/default/HOME/.dir_colors" "${HOME}/"
default "./misc/default/HOME/.zshrc" "${HOME}/"
default "./misc/default/HOME/.rtorrent.rc" "${HOME}/"

firefox "./misc/firefox/chrome" "./"
firefox "./misc/firefox/user.js" "./"
firefox "./misc/firefox/searchplugins" "./"

